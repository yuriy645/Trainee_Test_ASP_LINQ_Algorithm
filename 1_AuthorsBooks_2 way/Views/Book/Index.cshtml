@model List<Book>
    <p>The List of books:</p>
    <div class="outItem">
        @foreach (var book in @Model)
        {
            <div class="item">
                <div>
                    <b>@book.BookId</b>
                </div>
                <div>
                    @book.BookName
                </div>
            </div>
        }
    </div>
